<launch>
    <arg name="role_name" default="hero"/>
    <arg name="objects_definition_file" default="/home/ailab/carla-ros-bridge/catkin_ws/src/ros-bridge/carla_spawn_objects/config/objects.json"/>
    <arg name='synchronous_mode_wait_for_vehicle_control_command' default='False'/>
    
    <include file="/home/ailab/carla-ros-bridge/catkin_ws/src/ros-bridge/carla_spawn_objects/launch/carla_spawn_objects.launch">
        <arg name="objects_definition_file" value="$(arg objects_definition_file)"/>
        <arg name="spawn_point_ego_vehicle" value=""/>
        <arg name="spawn_sensors_only" value="True"/>
    </include>

    <include file="$(find carla_manual_control)/launch/carla_manual_control.launch">
        <arg name='role_name' value='$(arg role_name)'/>
    </include>

    <!-- <node pkg="team2_package" type="keyboard_control" name="keyboard_control" output="screen"/> -->

</launch>
